    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BatMan">
        <activity
            android:name=".share.AddStockActivity"
            android:exported="false" />
        <activity android:name=".share.AddTransactionActivity" />
        <activity android:name=".sellmode.sellingprice.SellingPriceTableModifyActivity" />
        <activity android:name=".sellmode.SellMainActivity" />
        <activity android:name=".managemode.stock.StockTableModifyActivity" />
        <activity android:name=".managemode.transaction.TransactionTableModifyActivity" />
        <activity android:name=".selectmode.SelectModeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".managemode.ManageMainActivity" />
    </application>

</manifest>